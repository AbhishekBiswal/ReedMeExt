{
  "name": "ReedMe",
  "description": "View any webpage or blog post in a cleaner, simpler, readable format on reedme.in",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["onclick.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page readable.",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}